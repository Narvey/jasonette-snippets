    "New Jasonette App": {
        "prefix": "jason",
        "body": [
            "{",
            "\t\"\\$jason\": {",
            "\t\t\"head\": {",
            "\t\t\t\"title\": \"${1:My New App}\",",
            "\t\t\t\"description\": \"${2:A jasonette app}\",",
            "\t\t\t\"offline\": \"${3|true,yes,on|}\",",
            "\t\t\t\"styles\": { },",
            "\t\t\t\"actions\": { },",
            "\t\t\t\"templates\": { },",
            "\t\t\t\"data\": { }",
            "\t\t},",
            "\t\t\"body\": {",
            "\t\t\t\"header\": { },",
            "\t\t\t\"sections\": [ ],",
            "\t\t\t\"layers\": [ ],",
            "\t\t\t\"footer\": { }",
            "\t\t}",
            "\t}",
